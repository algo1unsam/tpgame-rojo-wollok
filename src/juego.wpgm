import wollok.game.*
import plantas.*
import zombies.*

program juego {
	game.title("Plantoides vs Zomboides")
	game.addVisualCharacter(selector)
	game.height(5)
	game.width(15)
	game.ground("background.jpg")
	
	
	game.onTick(10000,"inicializadorZombie",{ => inicializadorZombie.agregarZombie()})
	
	
	
	
	//teclado
	keyboard.a().onPressDo{
		selector.plantar()
		
	}
	
	game.start()
}
